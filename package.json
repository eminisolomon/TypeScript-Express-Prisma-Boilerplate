{
  "name": "typescript-express-prisma",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Solomon Olatunji",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky install",
    "prebuild": "yarn run clean:build",
    "start": "yarn run build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "prod": "yarn run build && yarn run start",
    "lint": "eslint --ignore-path .eslintignore ./ --ext .ts,.js",
    "lint:fix": "eslint --ignore-path .eslintignore ./ --fix --ext .ts,.js",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(ts|js)\"",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "helmet": "^7.0.0",
    "jwk-to-pem": "^2.0.5",
    "jwks-rsa": "^3.1.0",
    "morgan": "^1.10.0",
    "nocache": "^4.0.0",
    "nodemon": "^3.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.1.0",
    "@swc/core": "^1.3.92",
    "@swc/helpers": "^0.5.2",
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.2",
    "@types/superagent": "^4.1.19",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^48.0.1",
    "express-jsdoc-swagger": "^1.8.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "node-notifier": "^10.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "description": "express",
  "repository": "https://github.com/solomonolatunji/Creemo-API.git",
  "private": false
}
